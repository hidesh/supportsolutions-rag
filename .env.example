# OpenAI API Konfiguration (PÅKRÆVET)
# Få din nøgle fra: https://platform.openai.com/api-keys
OPENAI_API_KEY=sk-proj-DIN-OPENAI-API-NOEGLE-HER

# AI Model Konfiguration
EMBED_MODEL=text-embedding-3-large
CHAT_MODEL=gpt-4o

# RAG System Konfiguration
TOP_K=6
CHUNK_SIZE_CHARS=600
CHUNK_OVERLAP_CHARS=100
REQUEST_TIMEOUT_SEC=60

# Server Konfiguration (valgfri)
API_HOST=127.0.0.1
API_PORT=8000
DEBUG=False

# SIKKERHEDSNOTAT:
# - Regenerer dine API-nøgler hvis de bliver kompromitteret
# - Overvåg dit API forbrug på OpenAI's platform
# - Brug forskellige nøgler til udvikling og production